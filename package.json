    "node",
    "js",
    "javascript",
    "SHA-256",
    "base64",
    "encode",
    "cli",
    "digest"
  ],
  "author": "Berkmann18",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Berkmann18/hashmyjs/issues"
  },
  "homepage": "https://github.com/Berkmann18/hashmyjs#readme",
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.12.2",
    "external-ip": "^1.3.1"
  },
  "devDependencies": {
    "complexity-report": "^2.0.0-alpha",
    "eslint": "^4.14.0",
    "eslint-plugin-security": "^1.4.0",
    "nsp": "^3.1.0"
  }
