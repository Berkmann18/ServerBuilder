{
  "name": "serverbuilder",
  "version": "1.0.0",
  "description": "A simple NodeJS/Express server builder",
  "main": "serverbuilder.js",
  "scripts": {
    "start": "node index.js",
    "debug": "DEBUG=true node index.js",
    "test": "mocha --exit && codecov",
    "doc": "jsdoc -c ./config/.jsdocrc",
    "lint": "eslint -c ./config/.eslintrc.js *.js src/*.js --fix",
    "cc": "cr -c ./config/.complexrc",
    "cc:all": "npm run cc *.js src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Berkmann18/hashmyjs.git"
  },
  "keywords": [
    "hash",
    "integrity",
    "node",
    "js",
    "javascript",
    "SHA-256",
    "base64",
    "encode",
    "cli",
    "digest"
  ],
  "author": "Berkmann18",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Berkmann18/hashmyjs/issues"
  },
  "homepage": "https://github.com/Berkmann18/hashmyjs#readme",
  "dependencies": {
    "colors": "^1.3.1",
    "external-ip": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "complexity-report": "^2.0.0-alpha",
    "eslint": "^5.3.0",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^5.2.0",
    "request": "^2.88.0"
  }
}