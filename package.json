{
  "name": "serverbuilder",
  "version": "0.1.0",
  "description": "A simple NodeJS/Express server builder",
  "main": "serverbuilder.js",
  "scripts": {
    "start": "node index.js",
    "debug": "DEBUG=true node index.js",
    "test": "eslint -c ./config/.eslintrc.js *.js src/*.js",
    "doc": "jsdoc -c ./config/.jsdocrc",
    "fmt": "eslint -c ./config/.eslintrc.js *.js src/*.js --fix",
